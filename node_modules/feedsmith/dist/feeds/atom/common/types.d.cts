//#region src/feeds/atom/common/types.d.ts
declare namespace Atom {
  type Text = string;
  type Link<TDate extends DateLike> = {
    href: string;
    rel?: string;
    type?: string;
    hreflang?: string;
    title?: string;
    length?: number;
    thr?: ThrNs.Link<TDate>;
  };
  type Person = {
    name: string;
    uri?: string;
    email?: string;
    arxiv?: ArxivNs.Author;
  };
  type Category = {
    term: string;
    scheme?: string;
    label?: string;
  };
  type Generator = {
    text: string;
    uri?: string;
    version?: string;
  };
  type Source<TDate extends DateLike> = {
    authors?: Array<Person>;
    categories?: Array<Category>;
    contributors?: Array<Person>;
    generator?: Generator;
    icon?: string;
    id?: string;
    links?: Array<Link<TDate>>;
    logo?: string;
    rights?: Text;
    subtitle?: Text;
    title?: Text;
    updated?: TDate;
  };
